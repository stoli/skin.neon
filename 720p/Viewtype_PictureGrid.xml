    <control type="image">
      <posx>0</posx>
      <posy>0</posy>
      <width>1280</width>
      <height>720</height>
      <texture>music/pic_back.png</texture>
      <animation effect="slide" start="0,0" end="0,400" time="200" delay="0" condition="Control.HasFocus(8000)">Conditional</animation>
      <visible>Control.IsVisible(51)</visible>
    </control>
    <control type="wraplist" id="51">
      <posx>-70</posx>
      <posy>350</posy>
      <height>515</height>
      <width>1380</width>
      <onleft>51</onleft>
      <onright>51</onright>
      <onup>7000</onup>
      <ondown>8000</ondown>
      <!-- View Name = Showcase -->
      <viewtype label="$LOCALIZE[31063]">list</viewtype>
      <orientation>horizontal</orientation>
      <focusposition>4</focusposition>
      <scrolltime>300</scrolltime>
      <preloaditems>2</preloaditems>
      <include>Animation_ZoomInZoomOut</include>
      <animation effect="slide" start="0,0" end="0,400" time="600" delay="0" condition="Control.HasFocus(8000)">Conditional</animation>
      <itemlayout width="150" height="245">
        <control type="group">
          <posx>0</posx>
          <posy>180</posy>
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <width>140</width>
            <height>140</height>
            <aspectratio align="center" aligny="bottom">keep</aspectratio>
            <texture background="true">$INFO[ListItem.Icon]</texture>
            <bordertexture border="5">button-nofocus.png</bordertexture>
            <bordersize>5</bordersize>
            <include>Animation_HiddenByInfo</include>
          </control>
          <control type="image">
            <posx>0</posx>
            <posy>142</posy>
            <width>140</width>
            <height>140</height>
            <aspectratio align="center" aligny="top">keep</aspectratio>
            <texture background="true" flipy="true">$INFO[ListItem.Icon]</texture>
            <bordertexture border="5">button-nofocus.png</bordertexture>
            <bordersize>5</bordersize>
            <animation effect="fade" start="30" end="30" time="0" condition="true">Conditional</animation>
            <include>Animation_HiddenByInfo</include>
          </control>
        </control>
      </itemlayout>
      <focusedlayout width="250" height="500">
        <control type="group">
          <posx>0</posx>
          <posy>90</posy>
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <width>250</width>
            <height>250</height>
            <aspectratio align="center" aligny="bottom">keep</aspectratio>
            <texture background="true">$INFO[ListItem.Icon]</texture>
            <bordertexture border="5">pic_border.png</bordertexture>
            <bordersize>5</bordersize>
            <animation type="Focus" reversible="false">
              <effect type="zoom" center="80,238" start="81" end="100" time="400" tween="back" easing="out" />
            </animation>
            <include>Animation_HiddenByInfo</include>
          </control>
          <control type="image">
            <posx>0</posx>
            <posy>252</posy>
            <width>250</width>
            <height>250</height>
            <aspectratio align="center" aligny="top">keep</aspectratio>
            <texture background="true" flipy="true">$INFO[ListItem.Icon]</texture>
            <bordertexture border="5">pic_border.png</bordertexture>
            <bordersize>5</bordersize>
            <animation type="Focus" reversible="false">
              <effect type="zoom" center="80,238" start="81" end="100" time="400" tween="back" easing="out" />
            </animation>
            <animation effect="fade" start="30" end="30" time="0" condition="true">Conditional</animation>
            <include>Animation_HiddenByInfo</include>
          </control>
        </control>
      </focusedlayout>
    </control>
    <control type="button" id="8000">
      <label>SHOULD NOT SEE ME</label>
      <onup>51</onup>
      <ondown>51</ondown>
      <onleft>51</onleft>
      <onright>51</onright>
      <width>500</width>
      <visible allowhiddenfocus="true">false</visible>
    </control>
    <control type="label">
      <description>Title txt</description>
      <posx>0</posx>
      <posy>10</posy>
      <width>1100</width>
      <height>15</height>
      <label>$INFO[ListItem.Label]</label>
      <align>left</align>
      <aligny>center</aligny>
      <font>Font_Neon_44</font>
      <textcolor>mainblue</textcolor>
      <shadowcolor>black</shadowcolor>
      <include>Animation_BackdropFade</include>
      <visible>Control.IsVisible(51)</visible>
    </control>
  </include>
  <include name="PictureThumbView">
    <control type="panel" id="58">
      <posx>40</posx>
      <posy>80</posy>
      <width>432</width>
      <height>560</height>
      <onleft>7000</onleft>
      <onright>60</onright>
      <onup>58</onup>
      <ondown>58</ondown>
      <viewtype label="Grid">list</viewtype>
      <pagecontrol>60</pagecontrol>
      <scrolltime>200</scrolltime>
      <preloaditems>2</preloaditems>
      <itemlayout height="134" width="134">
        <control type="image">
          <posx>10</posx>
          <posy>10</posy>
          <width>124</width>
          <height>124</height>
          <aspectratio>keep</aspectratio>
          <texture background="true" diffuse="common/thumb_matte.png">$INFO[ListItem.Icon]</texture>
          <bordertexture border="6">common/thumb_border.png</bordertexture>
          <fadetime>400</fadetime>
          <bordersize>6</bordersize>
        </control>
      </itemlayout>
      <focusedlayout height="134" width="134">
        <control type="image">
          <posx>10</posx>
          <posy>10</posy>
          <width>124</width>
          <height>124</height>
          <aspectratio>keep</aspectratio>
          <texture background="true">$INFO[ListItem.Icon]</texture>
          <bordertexture border="8">common/thumb_border_glow.png</bordertexture>
          <fadetime>400</fadetime>
          <bordersize>8</bordersize>
          <animation type="Focus" reversible="false">
            <effect type="zoom" center="auto" start="51" end="130" time="600" tween="back" easing="out" />
          </animation>
        </control>
      </focusedlayout>
    </control>
    <control type="scrollbar" id="60">
      <posx>480</posx>
      <posy>80</posy>
      <width>25</width>
      <height>576</height>
      <texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
      <texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
      <texturesliderbarfocus border="0,14,0,14">ScrollBarV_bar_focus.png</texturesliderbarfocus>
      <textureslidernib>ScrollBarNib.png</textureslidernib>
      <textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
      <onleft>510</onleft>
      <onright>2</onright>
      <showonepage>false</showonepage>
      <orientation>vertical</orientation>
      <visible>Control.IsVisible(511)</visible>
    </control>
    <control type="group">
      <posx>540</posx>
      <posy>60</posy>
      <visible>Control.IsVisible(58)</visible>
      <control type="label">
        <description>Title txt</description>
        <posx>0</posx>
        <posy>20</posy>
        <width>700</width>
        <height>15</height>
        <label>$INFO[ListItem.Label]</label>
        <align>center</align>
        <aligny>center</aligny>
        <font>Font_Neon_44</font>
        <textcolor>mainblue</textcolor>
        <shadowcolor>black</shadowcolor>
      </control>
      <control type="image">
        <posx>-9</posx>
        <posy>50</posy>
        <width>700</width>
        <height>500</height>
        <texture background="true" diffuse="common/big_picture_blur_layout.png">$INFO[ListItem.FilenameAndPath]</texture>
        <fadetime>400</fadetime>
        <aspectratio>keep</aspectratio>
      </control>
      <control type="label">
        <description>Date time txt</description>
        <posx>0</posx>
        <posy>570</posy>
        <width>700</width>
        <height>15</height>
        <label>$INFO[ListItem.PictureDateTime,[COLOR=mainblue]$LOCALIZE[31326][/COLOR] - ]</label>
        <align>center</align>
        <aligny>center</aligny>
        <font>Font_Neon_20</font>
        <textcolor>white</textcolor>
        <shadowcolor>black</shadowcolor>
      </control>
      <control type="label">
        <description>Resolution txt</description>
        <posx>0</posx>
        <posy>600</posy>
        <width>700</width>
        <height>15</height>
        <label>$INFO[ListItem.PictureResolution,[COLOR=mainblue]$LOCALIZE[31327][/COLOR] - ]</label>
        <align>center</align>
        <aligny>center</aligny>
        <font>Font_Neon_20</font>
        <textcolor>white</textcolor>
        <shadowcolor>black</shadowcolor>
      </control>
    </control>
  </include>
</includes>